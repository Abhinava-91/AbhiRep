{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rgmarchadf"
		},
		"ds_adlsgen2sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ds_adlsgen2sink'"
		},
		"ls_azuresql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azuresql'"
		},
		"ls_blobsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobsource'"
		},
		"ls_blobtoadls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobtoadls'"
		},
		"ls_gen2sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_gen2sink'"
		},
		"ls_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sink'"
		},
		"ls_sqlsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqlsource'"
		},
		"ds_adlsgen2sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rgmarchadlsgen2.dfs.core.windows.net"
		},
		"ls_gen2sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rgmarchadlsgen2.dfs.core.windows.net"
		},
		"ls_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rgmarchadlsgen2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rgmarchadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_adlsgen2sink_accountKey": {
						"value": "[parameters('ds_adlsgen2sink_accountKey')]"
					},
					"ls_azuresql_connectionString": {
						"value": "[parameters('ls_azuresql_connectionString')]"
					},
					"ls_blobsource_connectionString": {
						"value": "[parameters('ls_blobsource_connectionString')]"
					},
					"ls_blobtoadls_connectionString": {
						"value": "[parameters('ls_blobtoadls_connectionString')]"
					},
					"ls_gen2sink_accountKey": {
						"value": "[parameters('ls_gen2sink_accountKey')]"
					},
					"ls_sink_accountKey": {
						"value": "[parameters('ls_sink_accountKey')]"
					},
					"ls_sqlsource_connectionString": {
						"value": "[parameters('ls_sqlsource_connectionString')]"
					},
					"ds_adlsgen2sink_properties_typeProperties_url": {
						"value": "[parameters('ds_adlsgen2sink_properties_typeProperties_url')]"
					},
					"ls_gen2sink_properties_typeProperties_url": {
						"value": "[parameters('ls_gen2sink_properties_typeProperties_url')]"
					},
					"ls_sink_properties_typeProperties_url": {
						"value": "[parameters('ls_sink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}